{
    "name": "web-id/breadcrumb",
    "description": "Make breadcrumbs using a route macro",
    "keywords": [
        "breadcrumb",
        "laravel"
    ],
    "homepage": "https://github.com/web-id-fr/breadcrumb",
    "license": "MIT",
    "authors": [
        {
            "name": "Charley Guenier",
            "email": "charley@web-id.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2|^8.3",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0|^11.0",
        "spatie/laravel-navigation": "^1.1|^1.2",
        "inertiajs/inertia-laravel": "^0.6.0|^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "larastan/larastan": "^2.0",
        "orchestra/testbench": "^9.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "WebId\\Breadcrumb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebId\\Breadcrumb\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "stan": "phpstan --memory-limit=4G"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WebId\\Breadcrumb\\BreadcrumbServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
